{
  "tailwindCSS.experimental.classRegex": [
    ["clsx\\(([^)]*)\\)"],
    ["cva\\(([^)]*)\\)", "[\"'`]([^\"'`]*).*?[\"'`]"],
    ["className:\\s*['\"]([^'\"]*)['\"]"],
    ["className:\\s*[`]([^`]*)[`]"],
    ["className:\\s*cn\\(([^)]*)\\)"],
    ["className:\\s*\\{([^}]*)\\}"]
  ],
  "cSpell.ignorePaths": [
    "node_modules",
    "package-lock.json",
    "tailwind.config.ts"
  ],
  "cSpell.words": [
    "Kolumbus",
    "Combobox",
    "Typeahead",
    "belanosima",
    "kolumblue",
    "macintel",
    "macppc",
    "pkey",
    "tailwindcss",
    "UPLOADTHING"
  ]
}
