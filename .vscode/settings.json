{
  "tailwindCSS.experimental.classRegex": [
    ["clsx\\(([^)]*)\\)"],
    ["cva\\(([^)]*)\\)", "[\"'`]([^\"'`]*).*?[\"'`]"],
    ["className:\\s*['\"]([^'\"]*)['\"]"],
    ["className:\\s*[`]([^`]*)[`]"],
    ["className:\\s*cn\\(([^)]*)\\)"],
    ["className:\\s*\\{([^}]*)\\}"]
  ],
  "cSpell.ignoreWords": [
    "Appdata",
    "Combobox",
    "Kolumbus",
    "Typeahead",
    "belanosima",
    "datasources",
    "datetime",
    "extralight",
    "kolumblue",
    "macintel",
    "macppc",
    "neondatabase",
    "paralleldrive",
    "pkey",
    "tailwindcss",
    "tanstack",
    "trpc"
  ],
  "cSpell.ignorePaths": [
    "package-lock.json",
    "node_modules",
    "vscode-extension",
    ".git/objects",
    ".vscode",
    ".vscode-insiders",
    "tailwind.config.ts"
  ]
}
